{
    "name": "yii-tools/skeleton-app",
    "type": "library",
    "description": "Skeleton applications for YiiFramework v. 3.0.",
    "keywords": [
        "yii-tools",
        "skeleton",
        "applications",
        "yii3"
    ],
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "composer/installers": "^2.2",
        "psr/container": "^2.0",
        "psr/http-message": "^1.1",
        "psr/http-server-handler": "^1.0",
        "psr/log": "^3.0",
        "vlucas/phpdotenv": "^5.5",
        "yii-tools/service": "^1.0@dev",
        "yiisoft/aliases": "^3.0",
        "yiisoft/config": "^1.3",
        "yiisoft/definitions": "^3.2",
        "yiisoft/log": "^2.0",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/router": "^3.0",
        "yiisoft/router-fastroute": "^3.0",
        "yiisoft/session": "^2.0",
        "yiisoft/translator-message-php": "^1.1",
        "yiisoft/yii-console": "^2.1",
        "yiisoft/yii-runner-console": "^2.0",
        "yiisoft/yii-runner-http": "^2.0"
    },
    "require-dev": {
        "codeception/c3": "^2.7",
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "codeception/module-cli": "^2.0",
        "codeception/module-phpbrowser": "^3.0",
        "httpsoft/http-message": "^1.1",
        "maglnet/composer-require-checker": "^4.6",
        "symfony/process": "^6.3",
        "vimeo/psalm": "^5.9"
    },
    "autoload": {
        "psr-4": {
            "Yii\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        },
        "config-plugin-file": "/config/config-plugin.php",
        "config-plugin-options": {
            "source-directory": "config"
        }
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "yiisoft/config": true,
            "composer/installers": true,
            "codeception/c3": true
        },
        "sort-packages": true
    },
    "scripts": {
        "check-dependencies": "composer-require-checker",
        "mutation": "roave-infection-static-analysis-plugin",
        "psalm": "psalm",
        "test": "phpunit"
    }
}
